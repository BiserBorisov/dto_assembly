@model DtoDeepDive.Data.DTO.PartCatalogDTO
@{
    ViewBag.Title = "Home Page";
}
<div class="container">
  <h2>Part Catalog</h2>
  <br />
  @using (Html.BeginForm("Quote", "Home")) {
      <div class="row">
        <table class="table table-striped table-bordered">
          <thead>
            <tr>
              <th></th>
              <th>Part #</th>
              <th>Extended Description</th>
              <th>Quantity</th>
            </tr>
          </thead>
          <tbody>
            @Html.EditorFor(x => x.Parts)
          </tbody>
        </table>
        <button class="btn btn-default" type="submit">Generate Quote</button>
      </div>
  }
</div>