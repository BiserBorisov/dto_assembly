@model DtoDeepDive.Data.DTO.PartCatalogDTO
@{
    ViewBag.Title = "Home Page";
}
<div class="container">
  <br/>
  <div class="row">
    <table class="table table-striped table-bordered">
      <thead>
      <tr>
        <th>Part #</th>
      </tr>
      </thead>
      <tbody>
      @if (Model.Parts != null) {
        foreach (var partDto in Model.Parts) {
          <tr>@partDto.PartNumber</tr>
        }
      }
      </tbody>
    </table>
  </div>
</div>